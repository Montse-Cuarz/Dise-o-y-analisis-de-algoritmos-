cont =0;
for horas in range (0,24):
  transhoras=str(horas).zfill(2)
  for minutos in range (0,60):
    transminutos=str(minutos).zfill(2)
    if(transhoras == transminutos[::-1]):
      cont=cont+1;
      print(transhoras,":",transminutos)
      
print("Los palindromos en las horas del d√≠a son: ",cont)
